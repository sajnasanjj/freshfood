<div class="container">
  <div class="main-body pt-5">
    <div class="row gutters-sm">
      <div class="col-md-4 mb-3">
        <div class="card">
          <div class="card-body">
            <div class="d-flex flex-column align-items-center text-center">
              <img src="https://bootdey.com/img/Content/avatar/avatar7.png" alt="Admin" class="rounded-circle"
                width="150">
              <div class="mt-3">
                <h4>{{userDetailss.Name}}</h4>

                <p class="text-secondary">Referal Link :
                <h6>{{referLink}}</h6>
                </p>
              </div>
            </div>
          </div>
        </div>
        <br>
        <a href="/addAddress"> <button class="btn btn-outline-danger">Add Address</button></a>

      </div>
      <div class="col-md-8">
        <div class="card mb-3">
          <div class="card-body">
            <div class="row">
              <div class="col-sm-3">
                <h6 class="mb-0">Full Name</h6>
              </div>
              <div class="col-sm-9 text-secondary">
                {{userDetailss.Name}}
              </div>
            </div>
            <hr>

            <div class="row">
              <div class="col-sm-3">
                <h6 class="mb-0">Email</h6>
              </div>
              <div class="col-sm-9 text-secondary">
                {{userDetailss.Email}}
              </div>
            </div>
            <hr>
            <div class="row">
              <div class="col-sm-3">
                <h6 class="mb-0">Mobile</h6>
              </div>
              <div class="col-sm-9 text-secondary">
                {{userDetailss.Mobile}}
              </div>
            </div>
            <hr>


            <div class="row">
              <div class="col-sm-1">
                <a class="btn btn-outline-warning" target="__blank" href="/edit-profile/{{user._id}}">Edit</a>
              </div>
              <div class="col-sm-3">
                <a class="btn btn-outline-warning" target="__blank" href="/changePassword/{{user._id}}">Change
                  Password</a>
              </div>

            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="row gutters-sm">

        {{#each address}}
        <div class="col-sm-6 mb-3">

          <div class="card h-100">
            <div class="card-body">
              <h5 class="d-flex align-items-center mb-3"><i class="material-icons text-warning mr-2">1</i>Address</h5>
              <hr>
              <div class="row">

                <div class="col-sm-4">
                  <h6 class="mb-0">Flat</h6>
                </div>
                <div class="col-sm-8 text-secondary mb-3">
                  <p>{{this.Flat}}</p>
                </div>
              </div>
              <div class="row">
                <div class="col-sm-4 mb-3">
                  <h6 class="mb-0">Place</h6>
                </div>
                <div class="col-sm-8 text-secondary mb-3">
                  <p>{{this.Place}}</p>
                </div>
              </div>
              <div class="row">
                <div class="col-sm-4 mb-3">
                  <h6 class="mb-0">Street</h6>
                </div>
                <div class="col-sm-8 text-secondary mb-3">
                  <p>{{this.Street}}</p>
                </div>
              </div>
              <div class="row">
                <div class="col-sm-4 mb-3">
                  <h6 class="mb-0">Pincode</h6>
                </div>
                <div class="col-sm-8 text-secondary mb-3">
                  <p>{{this.Pincode}}</p>
                </div>
              </div>
              <div class="row">
                <div class="col-sm-4 mb-3">
                  <h6 class="mb-0">Mobile</h6>
                </div>
                <div class="col-sm-8 text-secondary mb-3">
                  <p>{{this.Mobile}}</p>
                </div>

              </div>
              <button class="btn btn-danger " onclick="sweetalertClick2('/delete-address/{{this._id}}')">
                Delete</button></td>

            </div>
          </div>


        </div>
        {{/each}}
      </div>

    </div>


  </div>
</div>

<script>
  function sweetalertClick2(value1) {
    Swal.fire({
      title: 'Are you sure?',
      text: "You won't be able to revert this!",
      icon: 'warning',
      showCancelButton: true,
      confirmButtonColor: '#3085d6',
      cancelButtonColor: '#d33',
      confirmButtonText: 'Yes, delete it!'
    }).then((result) => {
      if (result.isConfirmed) {
        {
          window.location = value1
        }
      }
    })
  }

</script>