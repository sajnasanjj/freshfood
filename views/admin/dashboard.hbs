<script type="text/javascript">
    var ctx = document.getElementById('myChart').getContext('2d');
    var myChart = new Chart(ctx, {
        type: 'pie',
        data: {
            labels: ['Pending Order', 'Success Order','Cancel Order'],
            datasets: [{
                label: 'Order methods',
                data: [{{data.pending}},{{data.totalOrderSuccess}},{{data.cancelOrder}}],
            backgroundColor: [
                'rgba(255, 99, 132, 1)',
                'rgba(54, 162, 235, 1)',
                
                'rgba(255, 0, 0, 1)',
            ],
        }]
    },
        options: {
        responsive: true,
    },
    
    });

    var earning = document.getElementById('earning').getContext('2d');
    var myChart = new Chart(earning, {
        type: 'bar',
        data: {
            labels: ['COD', 'PAYPAL', 'RAZORPAY'],
            datasets: [{
                label: 'Payment Methods',
                data: [{{data.COD}},{{data.paypal}},{{data.Razorpay}}],
            backgroundColor: [
                'rgba(255, 206, 86, 1)',
                'rgba(54, 162, 235, 1)',
                'rgba(60, 170, 70, 1)',

            ],
        }]
    },
        options: {
        responsive: true,
    }
    });
</script>

          

                        

          

